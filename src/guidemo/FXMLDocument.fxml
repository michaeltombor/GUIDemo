<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guidemo.FXMLDocumentController">
    <children>
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <HBox layoutX="-1.0" prefHeight="300.0" prefWidth="758.0">
         <children>
            <VBox prefHeight="300.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="CheckBox Objects">
                     <font>
                        <Font name="Arial Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <CheckBox fx:id="pepperoniCheckBox" mnemonicParsing="false" text="Pepperoni" />
                  <CheckBox fx:id="pineappleCheckBox" mnemonicParsing="false" text="Pineapple" />
                  <CheckBox fx:id="baconCheckBox" mnemonicParsing="false" text="Bacon" />
                  <Button mnemonicParsing="false" onAction="#pizzaOrderButtonPushed" text="Order Pizza" />
                  <Label fx:id="pizzaOrderLabel" alignment="TOP_LEFT" prefHeight="157.0" prefWidth="202.0" text="Label" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <VBox prefHeight="300.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label prefHeight="27.0" prefWidth="200.0" text="ChoiceBox Object">
                     <font>
                        <Font name="Arial Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ChoiceBox fx:id="choiceBox" prefWidth="150.0" />
                  <Button mnemonicParsing="false" onAction="#choiceBoxButtonPushed" text="Favorite Fruit" />
                  <Label fx:id="choiceBoxLabel" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <VBox prefHeight="290.0" prefWidth="202.0" spacing="10.0">
               <children>
                  <Label text="ComboBox Object">
                     <font>
                        <Font name="Arial Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="comboBox" onAction="#comboBoxWasUpdated" prefWidth="150.0" />
                  <Label fx:id="comboBoxLabel" text=" " />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <VBox prefHeight="300.0" prefWidth="225.0" spacing="10.0">
               <children>
                  <Label text="RadioButton Objects">
                     <font>
                        <Font name="Arial Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <RadioButton fx:id="pythonRadioButton" mnemonicParsing="false" onAction="#radioButtonPushed" text="Python" />
                  <RadioButton fx:id="javaRadioButton" mnemonicParsing="false" onAction="#radioButtonPushed" text="Java" />
                  <RadioButton fx:id="cSharpRadioButton" mnemonicParsing="false" onAction="#radioButtonPushed" text="C#" />
                  <RadioButton fx:id="javaScriptRadioButton" mnemonicParsing="false" onAction="#radioButtonPushed" text="JavaScript">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </RadioButton>
                  <Label fx:id="radioButtonLabel" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
      <HBox layoutX="1.0" layoutY="284.0" prefHeight="316.0" prefWidth="766.0" spacing="10.0">
         <children>
            <VBox prefHeight="250.0" prefWidth="200.0">
               <children>
                  <Label text="ListView Object">
                     <font>
                        <Font name="Arial Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ListView fx:id="listView" prefHeight="250.0" prefWidth="200.0" />
               </children>
            </VBox>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#listViewButtonPushed" text="Selected Gear" textAlignment="CENTER" />
               </children>
            </StackPane>
            <VBox prefHeight="250.0" prefWidth="200.0">
               <children>
                  <Label text="TextArea Object">
                     <font>
                        <Font name="Arial Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <TextArea fx:id="textArea" prefHeight="250.0" prefWidth="200.0" />
               </children>
            </VBox>
         </children>
      </HBox>
    </children>
